package edu.polytech.pojo;

import java.util.Date;

public class Session {

	public static User user;
	public static Date dateBegin;
	public static Date dateEnd;
	
	
	
	
	public static User getUser() {
		return user;
	}
	public static void setUser(User user) {
		Session.user = user;
	}
	public static Date getDateBegin() {
		return dateBegin;
	}
	public static void setDateBegin(Date dateBegin) {
		Session.dateBegin = dateBegin;
	}
	public static Date getDateEnd() {
		return dateEnd;
	}
	public static void setDateEnd(Date dateEnd) {
		Session.dateEnd = dateEnd;
	}
	
	
	
	
	
}
